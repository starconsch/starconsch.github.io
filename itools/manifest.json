{
  "_version": "1.58.0",
  "sap.app": {
    "id": "ch.starcons.itools",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "2.2.2"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.3",
      "toolsId": "e24708a7-4fee-48a4-a99d-006595dd74a1"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.119.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.unified": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ch.starcons.itools.i18n.i18n"
        }
      },
      "": {
          "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ch.starcons.itools.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "flexibleColumnLayout",
        "clearControlAggregation": false,
        "bypassed": {
            "target": [
                "error"
            ]
        }
      },
      "routes": [
        {
          "name": "Home",
          "pattern": "",
          "target": [
            "home"
          ]
        },
        {
          "name": "Launchpad",
          "pattern": "launchpad/layout/{layout}",
          "target": [
            "launchpad"
          ]
        },
        {
          "name": "CIReport",
          "pattern": "ciReport/layout/{layout}",
          "target": [
            "ciReport"
          ]
        },
        {
          "name": "PIReport",
          "pattern": "piReport/layout/{layout}",
          "target": [
            "piReport"
          ]
        },
        {
          "name": "PrivateFileSystem",
          "pattern": "privateFileSystem/layout/{layout}",
          "target": [
            "privateFileSystem"
          ]
        }
      ],
      "targets": {
        "home": {
          "viewType": "XML",
          "viewId": "viewHome",
          "viewName": "Home",
          "transition": "show"
        },
        "launchpad": {
          "viewType": "XML",
          "viewId": "viewLaunchpad",
          "viewName": "Launchpad",
          "transition": "show"
        },
        "ciReport": {
          "viewType": "XML",
          "viewId": "viewCIReport",
          "viewName": "CIReport",
          "transition": "show"
        },
        "piReport": {
          "viewType": "XML",
          "viewId": "viewPIReport",
          "viewName": "PIReport",
          "transition": "show"
        },
        "privateFileSystem": {
          "viewType": "XML",
          "viewId": "viewPrivateFileSystem",
          "viewName": "PrivateFileSystem",
          "transition": "show"
        },
        "error": {
          "viewType": "XML",
          "viewId": "viewError",
          "viewName": "Error",
          "transition": "show"
        }
      }
    },
    "rootView": {
      "viewName": "ch.starcons.itools.view.App",
      "type": "XML",
      "async": true,
      "id": "viewApp"
    }
  }
}
